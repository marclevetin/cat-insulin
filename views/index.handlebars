<h1>Cat Insulin Tracker</h1>
<h2>
  Record a dosage
</h2>
<form>
  <div class="form-row">
    <div class="form-group col">
      <label for="inputEmail4">Date</label>
      <input type="email" class="form-control" id="date" placeholder="Date">
    </div>
    <div class="form-group col">
      <label for="inputPassword4">Time</label>
      <input type="password" class="form-control" id="time" placeholder="Time">
    </div>
    <div class="form-group col">
      <label for="inputPassword4">Dosage</label>
      <input type="password" class="form-control" id="dosage" placeholder="Dosage">
    </div>
  </div>
  <button type="submit" class="btn btn-primary">Sign in</button>
</form>
<h2>Recent Dosages</h2>
<p>
  See all
</p>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Date</th>
      <th scope="col">Time</th>
      <th scope="col">Dosage</th>
    </tr>
  </thead>
  <tbody>
    {{#each dosages}}
      {{> dosages/dosage-row }}
    {{/each}}
  </tbody>
</table>
<p>
  Little graph
</p>
